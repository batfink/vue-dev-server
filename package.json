{
  "name": "vue-dev-server",
  "description": "a small development server for building `vue` components",
  "version": "0.0.1",
  "homepage": "https://github.com/paulpflug/",
  "author": {
    "name": "Paul Pflugradt",
    "email": "paul.pflugradt@gmail.com"
  },
  "license": "MIT",
  "bin": {
    "vue-dev-server": "./index.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/paulpflug/vue-dev-server"
  },
  "engines": {
    "node": "*"
  },
  "main": "lib/index.js",
  "dependencies": {
    "chokidar": "^1.4.2",
    "coffee-script": "^1.10.0",
    "commander": "^2.9.0",
    "ip": "^1.1.0",
    "koa": "^1.1.2",
    "koa-sendfile": "^1.1.1",
    "koa-static": "^2.0.0",
    "vue-hot-reload-api": "^1.3.2",
    "vue-router": "^0.7.10",
    "vueify-insert-css": "^1.0.0",
    "webpack": "^1.12.13",
    "webpack-hot-middleware": "^2.6.4",
    "webpack-koa-dev-middleware": "^0.1.3"
  },
  "devDependencies": {
    "jade": "^1.11.0",
    "rimraf": "^2.4.4"
  },
  "keywords": [
    "vue",
    "dev-server",
    "cli"
  ],
  "readmeFilename": "README.md",
  "scripts": {
    "clean": "rimraf lib/ && mkdir lib",
    "prebuild": "npm run clean",
    "build": "coffee --compile --output lib/ src/*.coffee",
    "watch": "coffee --compile --watch --output lib/ src/*.coffee",
    "patch": "npm version patch;npm publish;git push;git push --tags"
  }
}
